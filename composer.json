{
    "require": {
        "twig/twig": "^3.1",
        "symfony/console": "^7.1",
        "nesbot/carbon": "^3.6",
        "swoole/ide-helper": "^5.1",
        "vlucas/phpdotenv": "^5.6",
        "symfony/finder": "^7.1",
        "nikic/fast-route": "^1.3",
        "nunomaduro/termwind": "^2.0",
        "monolog/monolog": "^3.0",
        "psr/log": "^1.0|^2.0|^3.0",
        "symfony/mailer": "^7.1",
        "symfony/sendgrid-mailer": "^7.1",
        "ext-mbstring": "*",
        "ext-ffi": "*",
        "ext-swoole": "*",
        "cydrickn/socketio": "^1.6",
        "symfony/google-mailer": "^7.1",
        "giggsey/libphonenumber-for-php": "^8.13",
        "filp/whoops": "^2.16",
        "symfony/uid": "^7.2",
        "voku/portable-ascii": "^2.0",
        "doctrine/sql-formatter": "^1.5",
        "symfony/twig-bridge": "^7.2",
        "league/commonmark": "^2.6",
        "twig/extra-bundle": "^3.20",
        "twig/markdown-extra": "^3.12",
        "psr/event-dispatcher": "^1.0",
        "psr/container": "^2.0",
        "symfony/mailtrap-mailer": "^7.2",
        "league/flysystem": "^3.0",
        "php-ffmpeg/php-ffmpeg": "^1.3",
        "lcobucci/jwt": "^5.5",
        "lcobucci/clock": "^3.3"
    },
    "autoload": {
        "psr-4": {
            "Vietiso\\Core\\": "core"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vietiso\\Benchmark\\": "./benchmark"
        }
    },
    "require-dev": {
        "symfony/var-dumper": "^7.1",
        "phpunit/phpunit": "^11.4",
        "phpbench/phpbench": "^1.3"
    },
    "scripts": {
        "test": "php -d xdebug.mode=coverage ./vendor/bin/phpunit --coverage-html=coverage",
        "start": [
            "Composer\\Config::disableProcessTimeout",
            "php -dxdebug.mode=off vietiso start:serve"
        ],
        "start:watch": [
            "Composer\\Config::disableProcessTimeout",
            "php -dxdebug.mode=off vietiso start:serve --watch"
        ]
    }
}
